{
  "expo": {
    "name": "Zik",
    "slug": "zik-mobile-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/android/playstore-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/icons/ios/iTunesArtwork@2x.png"
    },
    "android": {
      "icon": "./assets/icons/android/mipmap-xxxhdpi/ic_launcher.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/android/mipmap-xxxhdpi/ic_launcher_foreground.png",
        "backgroundColor": "#F8F9FA"
      },
      "package": "com.zik.app"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/icons/ios/iTunesArtwork@2x.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-dev-client",
      [
        "expo-notifications",
        {
          "icon": "./assets/icons/android/mipmap-hdpi/ic_launcher.png",
          "color": "#ffffff",
          "sounds": []
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f49eb587-249d-44f3-8c0d-16d6812b4cd6"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/f49eb587-249d-44f3-8c0d-16d6812b4cd6"
    }
  }
}